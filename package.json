{
  "name": "template",
  "version": "1.0.0",
  "description": "A template for my basic Express apps",
  "main": "src/app.ts",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node dist/app.js",
    "dev": "tsx watch --ignore src/**/*.spec.ts --ignore e2e --clear-screen=false src/app.ts",
    "lint": "eslint . --report-unused-disable-directives",
    "format": "prettier --write ."
  },
  "keywords": [],
  "author": "lanceHoenhymmm",
  "license": "ISC",
  "dependencies": {
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/node": "^20.9.0",
    "@typescript-eslint/eslint-plugin": "^6.6.0",
    "@typescript-eslint/parser": "^6.6.0",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.0.0",
    "prettier": "^3.0.3",
    "tsx": "^4.1.1",
    "typescript": "^5.2.2"
  }
}
